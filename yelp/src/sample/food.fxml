<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FoodController">
   <children>
      <TableView fx:id="table" onMouseClicked="#goToBusiness" prefHeight="280.0" prefWidth="500.0" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="90.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="394.0" text="Business Name" />
          <TableColumn fx:id="starColumn" minWidth="2.0" prefWidth="105.0" text="Stars" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
