<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <GridPane layoutX="-4.0" layoutY="4.0" prefHeight="392.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="computers" mnemonicParsing="false" onAction="#nextPage" prefHeight="195.0" prefWidth="300.0" text="Computers" />
            <Button fx:id="home" mnemonicParsing="false" onAction="#nextPage" prefHeight="195.0" prefWidth="300.0" text="Home" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="fitness" mnemonicParsing="false" onAction="#nextPage" prefHeight="195.0" prefWidth="300.0" text="Fitness" GridPane.columnIndex="1" />
            <Button fx:id="discount" mnemonicParsing="false" onAction="#nextPage" prefHeight="195.0" prefWidth="300.0" text="Discount Stores" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
